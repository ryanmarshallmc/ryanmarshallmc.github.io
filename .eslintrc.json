{
  "parser": "babel-eslint",
  "plugins": ["prettier", "react-hooks"],
  "rules": {
    "prettier/prettier": [
      "warn",
      {
        "endOfLine": "auto"
      }
    ],
    "react-hooks/rules-of-hooks": "warn",
    "no-var": "warn",
    "prefer-const": "warn"
  },
  "extends": [
    "react-app",
    "plugin:prettier/recommended",
    "prettier",
    "prettier/react"
  ]
}
